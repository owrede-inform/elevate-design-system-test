  "scripts": {
    "dev": "gatsby develop",
    "dev:clean": "gatsby clean && gatsby develop -H 0.0.0.0",
    "dev:fast": "gatsby develop --preserve-cache",
    "build": "gatsby build",
    "build:clean": "yarn clean && gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "clean:images": "rimraf .cache/caches/gatsby-plugin-sharp && rimraf public/static",
    "render-images": "yarn clean:images && gatsby build --prefix-paths",
    "cache:info": "node scripts/cache-info.js",
    "cache:preserve": "echo 'Using existing image cache for faster builds' && gatsby develop --preserve-cache"
  },
