{
  "name": "elevate-design-system-test",
  "private": true,
  "version": "4.1.4",
  "license": "Apache 2.0",
  "engines": {
    "yarn": ">=1.22.0",
    "node": ">=18.0.0"
  },
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "dev": "node node_modules/gatsby/cli.js develop",
    "dev:clean": "powershell -ExecutionPolicy Bypass -File clean-dev.ps1",
    "dev:clean:reinstall": "powershell -ExecutionPolicy Bypass -File clean-dev.ps1 --reinstall",
    "dev:clean:simple": "powershell -ExecutionPolicy Bypass -File dev-clean-simple.ps1",
    "dev:fast": "echo 'Starting fast dev server (preserving image cache)' && node node_modules/gatsby/cli.js develop",
    "build": "node node_modules/gatsby/cli.js build",
    "build:clean": "yarn clean && node node_modules/gatsby/cli.js build",
    "serve": "node node_modules/gatsby/cli.js serve",
    "clean": "node node_modules/gatsby/cli.js clean",
    "clean:force": "powershell -ExecutionPolicy Bypass -Command \"if (Test-Path '.cache') { Remove-Item -Path '.cache' -Recurse -Force -ErrorAction SilentlyContinue }; if (Test-Path 'public') { Remove-Item -Path 'public' -Recurse -Force -ErrorAction SilentlyContinue }\"",
    "clean:images": "rimraf .cache/caches/gatsby-plugin-sharp && rimraf public/static",
    "render-images": "yarn clean:images && node node_modules/gatsby/cli.js build --prefix-paths",
    "cache:info": "node scripts/cache-info.js",
    "cache:preserve": "echo 'Using existing image cache for faster builds' && node node_modules/gatsby/cli.js develop --preserve-cache"
  },
  "dependencies": {
    "@carbon/icons-react": "^11.43.0",
    "@floating-ui/utils": "^0.2.8",
    "@mdi/js": "^7.4.47",
    "@shoelace-style/shoelace": "^2.20.1",
    "composed-offset-position": "^0.0.6",
    "gatsby": "^5.13.6",
    "gatsby-plugin-sass": "^6.13.1",
    "gatsby-theme-carbon": "^4.1.4",
    "lit": "^3.3.1",
    "qr-creator": "^1.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sass": "1.62.1"
  },
  "resolutions": {
    "sass": "1.62.1",
    "tr46": "^5.1.1",
    "whatwg-url": "^14.2.0",
    "core-js": "^3.44.0",
    "node-fetch/whatwg-url": "^14.2.0",
    "babel-runtime/core-js": "^3.44.0",
    "composed-offset-position": "^0.0.6"
  },
  "devDependencies": {
    "eslint-plugin-mdx": "^3.6.2",
    "gatsby-plugin-eslint": "^4.0.4",
    "rimraf": "^6.0.1"
  }
}
